{
  "name": "buexplain/netsvr-business",
  "description": "这是一个可以快速开发websocket业务的包，它基于https://github.com/buexplain/netsvr进行工作。",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "buexplain",
      "email": "buexplain@qq.com"
    }
  ],
  "keywords": [
    "php",
    "swoole",
    "hyperf",
    "websocket",
    "netsvr"
  ],
  "require": {
    "php": ">=8.1",
    "buexplain/netsvr-protocol-php": "dev-main",
    "hyperf/command": "3.0.x-dev",
    "hyperf/di": "3.0.x-dev",
    "hyperf/devtool": "3.0.x-dev",
    "hyperf/config": "3.0.x-dev",
    "hyperf/logger": "3.0.x-dev"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "NetsvrBusiness\\": "src/"
    }
  },
  "require-dev": {
    "swoole/ide-helper": "*",
    "friendsofphp/php-cs-fixer": "*",
    "hyperf/signal": "@dev",
    "google/protobuf": "^3.22"
  },
  "extra": {
    "hyperf": {
      "config": "NetsvrBusiness\\ConfigProvider"
    }
  }
}
